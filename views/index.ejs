<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link href="/css/styles.css" rel="stylesheet" />
    <link href="/css/coverShapes.css" rel="stylesheet" />
    <title>Document</title>
  </head>
  <body class="h-screen bg-[#FFF8F0]">

    <!-- NAVBAR -->

    <nav class="bg-[#9DD9D2] h-16 w-full flex justify-center items-center sticky top-0 text-lg shadow-md z-20">
      <div class=" h-12 w-full hidden md:flex justify-evenly items-center mx-32">
        <ul class="flex items-center">
          <Link to="home" smooth={true}>
            <li class="h-full px-4 cursor-pointer  hover:bg-[#FFF8F0] transition-all delay-75 duration-300 rounded-full">
              Home
            </li>
          </Link>
          <Link to="about" smooth={true}>
            <li class="h-full px-4 cursor-pointer hover:bg-[#FFF8F0] transition-all delay-75 duration-300 rounded-full whitespace-nowrap ">
              About Us
            </li>
          </Link>
          <Link to="package-container" smooth={true}>
            <li class="h-full px-4 cursor-pointer hover:bg-[#FFF8F0] transition-all delay-75 duration-300 rounded-full">
              Packages
            </li>
          </Link>
          <Link to="contact" smooth={true}>
            <li class="h-full px-4 cursor-pointer hover:bg-[#FFF8F0] transition-all delay-75 duration-300 rounded-full">
              Contact
            </li>
          </Link>
        </ul>
      </div>
    </nav>

    <!-- COVER -->

    <div class="h-full flex justify-center items-center -mt-16">
      <div id="wrapper" class="z-0 rotate-90 sm:rotate-0" data-configuration="1" data-roundness="1">
        <div class="shape"></div>
        <div class="shape"></div>
        <div class="shape"></div>
        <div class="shape"></div>
        <div class="shape"></div>
        <div class="shape"></div>
        <div class="shape"></div>
      </div>
      <img id="cover-image" class="z-10" src="/public/images/1057012.png" alt="#">
    </div>

  </body>
  <script src="./js/shapes.js"></script>
</html>
